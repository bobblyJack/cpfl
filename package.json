{
  "name": "@cpfl/office-addin",
  "description": "an office add-in designed for clark panagakos family law",
  "author": "LG-CPFL",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://lg-cpfl@dev.azure.com/lg-cpfl/office-addin/_git/office-addin"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "convert": "node scripts/manifest.js",
    "start": "office-addin-debugging start dist/manifest.xml desktop --packager build --no-debug",
    "stop": "office-addin-debugging stop dist/manifest.xml",
    "dev-server": "node scripts/server.js",
    "dev-verify": "office-addin-dev-certs install --machine"
  },
  "devDependencies": {
    "office-addin-cli": "*",
    "office-addin-debugging": "*",
    "office-addin-dev-certs": "*",
    "office-addin-dev-settings": "*",
    "office-addin-manifest": "*",
    "office-addin-node-debugger": "*",
    "typescript": "*",
    "@types/node": "*",
    "@types/office-js": "*"
  }
}

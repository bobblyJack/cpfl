{
  "name": "@cpfl/office-addin",
  "description": "an office add-in designed for clark panagakos family law",
  "author": "LG-CPFL",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://lg-cpfl@dev.azure.com/lg-cpfl/office-addin/_git/office-addin"
  },
  "scripts": {
    "dev-log": "office-addin-dev-settings runtime-log --enable",
    "dev-server": "node tasks/server.js",
    "sideload-start": "office-addin-debugging start dist/manifest.xml --no-debug",
    "sideload-stop": "office-addin-debugging stop dist/manifest.xml && office-addin-dev-settings runtime-log --disable",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "configure-sso": "office-addin-sso configure manifest.xml"
  },
  "config": {
    "app_to_debug": "word",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "dependencies": {
    "@azure/msal-browser": "*",
    "office-addin-sso": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/office-js": "*",
    "office-addin-cli": "*",
    "office-addin-debugging": "*",
    "office-addin-dev-certs": "*",
    "office-addin-dev-settings": "*",
    "office-addin-manifest": "*",
    "typescript": "*"
  }
}

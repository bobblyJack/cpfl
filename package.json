{
  "name": "@cpfl/office-addin",
  "description": "an office add-in designed for clark panagakos family law",
  "author": "LG-CPFL",
  "version": "1.0.0",

  "private": true,
  "license": "UNLICENSED",

  "repository": {
    "type": "git",
    "url": "https://lg-cpfl@dev.azure.com/lg-cpfl/office-addin/_git/office-addin"
  },

  "scripts": {
    "build": "node ./scripts/build.js",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "test": "office-addin-debugging dist/manifest.xml",
    "test:desktop": "office-addin-debugging dist/manifest.xml desktop",
    "test:web": "office-addin-debugging dist/manifest.xml web",
    "validate": "office-addin-manifest validate dist/manifest.xml"
  },

  "devDependencies": {
    "typescript": "latest",
    "@types/node": "latest",
    "@types/office-js": "latest",
    "office-addin-cli": "latest",
    "office-addin-manifest": "latest",
    "office-addin-debugging": "latest",
    "office-addin-dev-certs": "latest",
    "office-addin-dev-settings": "latest"
  }
}
